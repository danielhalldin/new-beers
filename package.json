{
  "name": "new-beers",
  "version": "0.1.63",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run build-service-worker",
    "build-service-worker": "node scripts/build_workbox && node scripts/inject_version_to_sw && node scripts/delete_default_sw",
    "test": "react-scripts test --env=jsdom",
    "lint": "eslint src --max-warnings 0",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm version patch"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "es6": true,
      "jest": true,
      "browser": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1,
      "react/prop-types": 0,
      "no-restricted-imports": [
        "error",
        {
          "paths": [
            {
              "name": "styled-components",
              "message": "Please import from styled-components/macro."
            }
          ],
          "patterns": [
            "!styled-components/macro"
          ]
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@apollo/react-hooks": "3.1.5",
    "@graphql-codegen/typescript-document-nodes": "1.14.0",
    "@types/jest": "25.2.3",
    "@types/js-cookie": "2.2.6",
    "@types/lodash": "4.14.152",
    "@types/node": "13.13.8",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/styled-components": "5.1.0",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link": "1.2.14",
    "apollo-link-http": "1.5.17",
    "graphql": "15.0.0",
    "graphql-tag": "2.10.3",
    "js-cookie": "2.2.1",
    "lodash": "4.17.15",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-apollo": "3.1.5",
    "react-device-detect": "1.12.1",
    "react-dom": "16.13.1",
    "react-fastclick-alt": "2.0.1",
    "react-image": "2.4.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "react-use": "15.1.0",
    "styled-components": "5.1.0",
    "typescript": "3.9.3",
    "workbox-build": "5.1.3"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "1.10.7",
    "eslint": "6.8.0",
    "eslint-plugin-react": "7.20.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.4"
  }
}
